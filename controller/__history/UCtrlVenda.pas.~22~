unit UCtrlVenda;

interface
uses UVenda,Classes;
type TCtrlVenda = class(TObject)

      private

      protected
        FUmaVenda:TVenda;

      public


        procedure SalvarVenda(vVenda:TVenda);

        function getPeriodoVenda(vPeriodoI:string; vPeriodoII:string):TList;
        procedure FecharCaixa(vPeriodo:string);
        procedure AbrirCaixa();

        constructor Criar;
        destructor Destruir;


end;

implementation

end.
